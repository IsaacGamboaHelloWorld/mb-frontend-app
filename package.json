{
  "name": "popular-banca-movil",
  "version": "3.6.4",
  "versionAndroid": "3.6.4",
  "versionIos": "3.6.4",
  "author": "Ionic Framework",
  "homepage": "https://ionicframework.com/",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "start:local": "ng run app:serve --proxy-config proxy.conf.json",
    "precommit": "npm run lint  && npm run prettier && npm run lint:html && npm run lint:sass",
    "ios:run:dev": "ionic build -c dev  && npx cap sync ios && npx cap open ios",
    "ios:run:stg": "ionic build -c stg  && npx cap sync ios && npx cap open ios",
    "ios:run:pro": "ionic build -c pro  && npx cap sync ios && npx cap open ios",
    "android:run:dev": "ionic build -c dev && npx cap sync android && npx cap open android",
    "android:run:stg": "ionic build -c stg && npx cap sync android && npx cap open android",
    "android:run:pro": "ionic build -c pro && npx cap sync android && npx cap open android",
    "android:live": "ionic capacitor run android -l --external --proxy-config proxy.conf.json",
    "compodoc": "npx compodoc -p ./tsconfig.app.json",
    "coverage": "ng test --watch=false --code-coverage && http-server coverage/ -o",
    "analyzer": "ng build -c dev --stats-json && webpack-bundle-analyzer www/stats-es2015.json",
    "lint:html": "htmlhint src/**/*.html",
    "prettier:list": "prettier --list-different './{e2e,src}/**/*.{scss,ts}' || ECHO .",
    "prettier:write": "prettier --write '{e2e,test-helpers,src}/**/*.ts'",
    "prettier:write:sass": "prettier-stylelint --write 'src/**/*.sass'",
    "lint:sass": "stylelint src/app/**/**/*.sass --syntax sass",
    "prettier": "prettier --check 'src/app/**/*.ts'",
    "sonar": "sonar-scanner",
    "apk:android": "ionic build -c dev && npx cap copy && cd android/ && ./gradlew assembleRelease",
    "apk:android:debugger": "ionic build -c dev && npx cap copy && cd android/ && ./gradlew assembleDebug",
    "resources": "cordova-res ios && cordova-res android && node scripts/resources.js",
    "postinstall": "jetifier"
  },
  "private": true,
